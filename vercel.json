{
    "version": 2,
    "env": {
        "CI": "true"
      },
      "command": "npm ci && npm run build",
    "builds": [
        
      {
        "src": "./src/index.js",
        "use": "@vercel/node"
      }
    ],
    
    "installCommand" : "npm ci",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }