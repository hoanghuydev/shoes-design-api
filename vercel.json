{
    "version": 2,
    "builds": [
        
      {
        "env": {
            "CI": "true"
          },
          "command": "npm i --force && npm run build",
        "src": "./src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }